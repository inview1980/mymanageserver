<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bootTest.pojo.LostbookEntity" table="lostbook" schema="book">
        <id name="bookId" column="BookID"/>
        <property name="lostUserCode" column="LostUserCode"/>
        <property name="lostDate" column="LostDate"/>
        <property name="parFor" column="ParFor"/>
    </class>
</hibernate-mapping>