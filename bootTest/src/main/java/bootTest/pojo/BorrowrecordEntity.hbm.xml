<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bootTest.pojo.BorrowrecordEntity" table="borrowrecord" schema="book">
        <id name="bookId" column="BookID"/>
        <property name="userCode" column="UserCode"/>
        <property name="borrowDate" column="BorrowDate"/>
        <property name="returnDate" column="ReturnDate"/>
        <property name="overTimeCost" column="OverTimeCost"/>
    </class>
</hibernate-mapping>